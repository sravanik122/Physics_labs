{
  "experiment_version": "1.0.0",

  "default_sampling_interval": 0.1,
  "default_total_time": 10,
  "noise_sigma": 0.015,

  "constants": {
    "gravity_default": 9.81,

    "surface_type_factor": {
      "soft": 0.6,
      "hard": 1.0,
      "elastic": 0.8
    },

    "contact_shape_factor": {
      "flat": 1.0,
      "circular": 1.2,
      "edge": 1.6,
      "pointed": 2.0
    },

    "material_hardness_factor": {
      "low": 0.5,
      "medium": 1.0,
      "high": 1.5
    },

    "pressure_distribution_factor": {
      "uniform": 1.0,
      "non_uniform": 1.3
    },

    "external_resistance_factor": {
      "low": 0.8,
      "medium": 1.0,
      "high": 1.3
    }
  },

  "visual": {
    "max_force_arrow_length": 120,
    "pressure_color_map": "thermal",
    "max_deformation_depth": 0.05,
    "show_normal_force_vector": true,
    "show_pressure_heatmap": true
  }
}
